            <TextBox Height="72" HorizontalAlignment="Left" Margin="320,119,0,0" MaxLength="4" Name="T_Oficina"  VerticalAlignment="Top" Width="180" Padding="10,2,10,5" FontWeight="Bold" BorderThickness="0" FontSize="50" />
