            <TextBox Height="72" HorizontalAlignment="Left" Margin="320,197,0,0" MaxLength="2" Name="T_DC" VerticalAlignment="Top" Width="90" Padding="10,2,10,5" FontWeight="Bold" BorderThickness="0" FontSize="50" />
