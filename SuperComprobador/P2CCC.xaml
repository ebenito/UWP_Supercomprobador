            <TextBox Height="72" HorizontalAlignment="Left" Margin="320,275,0,0" MaxLength="10" Name="T_CCC" VerticalAlignment="Top" Width="388" Padding="10,2,10,5" FontWeight="Bold" BorderThickness="0" FontSize="50" />
